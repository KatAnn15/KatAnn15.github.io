{
  "name": "prototype-parth-with-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.[contenthash].js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "start": "node server.js",
    "test": "jest --watch"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "verbose": true,
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      ".+\\.(css|sass|scss)$": "babel-jest"
    },
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./src/setupTests.ts"
    ],
    "automock": false,
    "resetMocks": false,
    "moduleNameMapper": {
      "^@images/(.*)$": "<rootDir>/src/assets/images/$1",
      "^@redux/(.*)$": "<rootDir>/src/scripts/Redux/$1",
      "^@constants/(.*)$": "<rootDir>/src/scripts/Constants/$1",
      "^@firebaseMy/(.*)$": "<rootDir>/src/scripts/Global/Firebase/$1",
      "^@test-utils": "<rootDir>/src/test-utils",
      "^@scriptsMy/(.*)$": "<rootDir>/src/scripts/$1",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
  "engines": {
    "node": "14.15.1",
    "npm": "6.14.11"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@firebase/rules-unit-testing": "^2.0.1",
    "@firebase/testing": "^0.20.11",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/google.maps": "^3.45.6",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "firebase-mock": "^2.3.2",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.6",
    "install": "^0.13.0",
    "jest": "^27.3.1",
    "jest-transform-scss": "^1.0.8",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "npm": "^7.24.0",
    "react-router-dom": "^5.3.0",
    "sass-loader": "^12.1.0",
    "scss-loader": "0.0.1",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.0.7",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/jsdom": "^16.2.13",
    "@types/node": "^16.9.4",
    "@types/react-redux": "^7.1.19",
    "@types/uuid": "^8.3.1",
    "axios": "^0.21.4",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-formidable-v2": "^2.2.0",
    "firebase": "^9.4.1",
    "fs": "^0.0.1-security",
    "google-maps-react": "^2.0.6",
    "https": "^1.0.0",
    "react": "^17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-stripe-checkout": "^2.6.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "stripe": "^8.176.0",
    "web-vitals": "^2.1.0",
    "webpack-merge": "^5.8.0"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
